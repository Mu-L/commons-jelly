<?xml version="1.0"?>

<j:jelly xmlns:j="jelly:core"
         xmlns:q="jelly:quartz">


  <q:job group="demo" name="cheese">
    I like cheese
  </q:job>

  <q:cron spec="*/5 * * ? * *"
          name="every.5.seconds"
          group="demo"
          jobName="cheese"
          jobGroup="demo"/>

  <q:wait-for-scheduler/>

</j:jelly>
