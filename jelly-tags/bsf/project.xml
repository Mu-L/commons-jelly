  <!ENTITY commonDeps SYSTEM "file:../../commonDependencies.ent">
]>
<project>
  <extend>${basedir}/../tag-project.xml</extend>
  <id>commons-jelly-tags-bsf</id>
  <name>commons-jelly-tags-bsf</name>
  <package>org.apache.commons.jelly.tags.bsf</package>

  <description>
